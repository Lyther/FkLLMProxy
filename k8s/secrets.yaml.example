# This is an example file. Create actual secrets using:
# kubectl create secret generic fkllmproxy-secrets \
#   --from-literal=master-key='sk-your-secret-key' \
#   --from-literal=google-credentials='<base64-encoded-json>'

apiVersion: v1
kind: Secret
metadata:
  name: fkllmproxy-secrets
  namespace: default
type: Opaque
stringData:
  master-key: "sk-your-secret-key-here"
  google-credentials: |
    {
      "type": "service_account",
      "project_id": "your-project-id",
      ...
    }

